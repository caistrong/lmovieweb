{
  "name": "lmovieweb",
  "version": "1.0.0",
  "description": "movie recommend system web server by caixiaocong",
  "main": "src/web.js",
  "scripts": {
    "start": "cross-env NODE_ENV=dev && node src/web.js",
    "deploy": "cross-env NODE_ENV=production && pm2 start src/web.js",
    "webmon": "cross-env NODE_ENV=dev && nodemon --watch src src/web.js",
    "gen-model": "node bin/build_model.js",
    "lint": "eslint src/",
    "apidoc": "apidoc -i src/router -o apidoc"
  },
  "directories": {
    "doc": "docs",
    "test": "test"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "ger": "0.0.99",
    "ger_mysql_esm": "0.0.2",
    "joi": "^14.3.1",
    "joi-date-extensions": "^1.2.0",
    "jsonwebtoken": "^8.5.1",
    "koa": "^2.7.0",
    "koa-bodyparser": "^4.2.1",
    "koa-compose": "^4.1.0",
    "koa-router": "^7.4.0",
    "koa-static": "^5.0.0",
    "lodash": "^4.17.11",
    "moment": "^2.24.0",
    "mysql2": "^1.6.5",
    "sequelize": "^4.42.0",
    "sequelize-auto": "^0.4.29",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "apidoc": "^0.17.7",
    "cross-env": "^5.2.0",
    "eslint": "^5.13.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.16.0",
